{
  "expo": {
    "name": "GOZeroWaste",
    "slug": "GOZeroWaste",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tsh22.gozerowaste",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes to allow users to borrow reusables and earn rewards from bringing their own reusables."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tsh22.gozerowaste",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/icons/favicon.png"
    }
  }
}
